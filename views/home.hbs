<h3>Student Managment System</h3>
<p><a href="/adduser" class="addbtn">Add User</a></p>
<table>
    <thead>
        <tr>
            <td>#</td>
            <td>Name</td>
            <td>Age</td>
            <td>City</td>
            <td>Edit</td>
            <td>Delete</td>
        </tr>
        <tbody>
            {{#each rows}}
            <tr>
                <td>{{this.ID}}</td>
                <td>{{this.NAME}}</td>
                <td>{{this.AGE}}</td>
                <td>{{this.CITY}}</td>
                <td><a class="edit" href="/edituser/{{this.ID}}">Edit</a></td>
                <td><a class="delete" href="/deleteuser/{{this.ID}}">Delete</a></td>
            </tr>
            {{/each}}
        </tbody>
    </thead>
</table>